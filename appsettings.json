{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",


  "FilterSettings": {
    "DomainNamePattern": "^(?:https?:\\/\\/(?:www\\.)?(?:[a-zA-Z0-9-]+\\.)*?([a-zA-Z0-9-]+)\\.[a-zA-Z]{2,}(?:\\.[a-zA-Z]{2,})*(?:\\/.*)?)$"
  }
}
